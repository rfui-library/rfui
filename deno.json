{
  "name": "@rfui-library/rfui",
  "version": "0.1.29",
  "tasks": {
    "typecheck": "deno check **/*.ts",
    "lint": "deno lint"
  },
  "imports": {
    "react": "https://esm.sh/react@18.2.0"
  },
  "exports": "./mod.ts",
  "publish": {
    "exclude": [
      ".github",
      ".vscode",
      ".gitignore",
      ".prettierignore",
      ".prettierrc"
    ]
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/react@18.2.0",
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns"]
  },
  "lint": {
    "rules": {
      "exclude": ["no-explicit-any"]
    }
  }
}
